        <item name="colorPrimary">@color/window_tide_midnight_blue</item>
        <item name="colorPrimaryVariant">@color/window_tide_dark_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/window_tide_baby_blue</item>
        <item name="colorSecondaryVariant">@color/window_tide_blue_gray</item>
